{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport defaultUserPhoto from '@/assets/photo/Potted Plant.svg';\nexport default {\n  name: 'NavBar',\n  setup() {\n    const userPhoto = ref('');\n    const userName = ref('');\n    const userId = ref('');\n    const fullUserPhotoUrl = computed(() => {\n      return userPhoto.value || defaultUserPhoto;\n    });\n    const fetchUserData = () => {\n      const tg = window.Telegram.WebApp;\n      tg.ready(() => {\n        const user = tg.initDataUnsafe.user;\n        if (user) {\n          userName.value = user.first_name || 'Імя клієнта';\n          userId.value = user.id || '';\n          loadUserPhoto(user.photo_url);\n          saveUserDataToLocalStorage(user.id, user.first_name, user.photo_url);\n\n          // Вивести всі дані, отримані з Telegram, у консоль\n          console.log('Received data from Telegram:', tg.initDataUnsafe);\n        }\n      });\n    };\n    const loadUserPhoto = photoUrl => {\n      if (photoUrl) {\n        userPhoto.value = photoUrl;\n      } else {\n        userPhoto.value = defaultUserPhoto;\n      }\n    };\n    const saveUserDataToLocalStorage = (id, name, photoUrl) => {\n      localStorage.setItem('telegram_user_id', id);\n      localStorage.setItem('telegram_user_name', name);\n      localStorage.setItem('telegram_user_photo_url', photoUrl);\n    };\n    onMounted(() => {\n      fetchUserData();\n    });\n    return {\n      fullUserPhotoUrl,\n      userName,\n      userId\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","defaultUserPhoto","name","setup","userPhoto","userName","userId","fullUserPhotoUrl","value","fetchUserData","tg","window","Telegram","WebApp","ready","user","initDataUnsafe","first_name","id","loadUserPhoto","photo_url","saveUserDataToLocalStorage","console","log","photoUrl","localStorage","setItem"],"sources":["C:\\Users\\Denys\\WebstormProjects\\kokh-assistant-vue-telegram\\src\\components\\app\\NavBar.vue"],"sourcesContent":["<template>\r\n  <div class=\"background-navbar\">\r\n    <div class=\"container-user-photo\">\r\n      <div class=\"user-photo\">\r\n        <img :src=\"fullUserPhotoUrl\" alt=\"user photo\">\r\n      </div>\r\n      <span class=\"container-user-name\">\r\n        {{ userName }} (ID: {{ userId }})\r\n      </span>\r\n    </div>\r\n    <div class=\"logotype-psychology\">\r\n      <img src=\"@/assets/photo/favicon-5.png\" alt=\"logotype\">\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from 'vue';\r\nimport defaultUserPhoto from '@/assets/photo/Potted Plant.svg';\r\n\r\nexport default {\r\n  name: 'NavBar',\r\n  setup() {\r\n    const userPhoto = ref('');\r\n    const userName = ref('');\r\n    const userId = ref('');\r\n\r\n    const fullUserPhotoUrl = computed(() => {\r\n      return userPhoto.value || defaultUserPhoto;\r\n    });\r\n\r\n    const fetchUserData = () => {\r\n      const tg = window.Telegram.WebApp;\r\n\r\n      tg.ready(() => {\r\n        const user = tg.initDataUnsafe.user;\r\n        if (user) {\r\n          userName.value = user.first_name || 'Імя клієнта';\r\n          userId.value = user.id || '';\r\n          loadUserPhoto(user.photo_url);\r\n          saveUserDataToLocalStorage(user.id, user.first_name, user.photo_url);\r\n\r\n          // Вивести всі дані, отримані з Telegram, у консоль\r\n          console.log('Received data from Telegram:', tg.initDataUnsafe);\r\n        }\r\n      });\r\n    };\r\n\r\n    const loadUserPhoto = (photoUrl) => {\r\n      if (photoUrl) {\r\n        userPhoto.value = photoUrl;\r\n      } else {\r\n        userPhoto.value = defaultUserPhoto;\r\n      }\r\n    };\r\n\r\n    const saveUserDataToLocalStorage = (id, name, photoUrl) => {\r\n      localStorage.setItem('telegram_user_id', id);\r\n      localStorage.setItem('telegram_user_name', name);\r\n      localStorage.setItem('telegram_user_photo_url', photoUrl);\r\n    };\r\n\r\n    onMounted(() => {\r\n      fetchUserData();\r\n    });\r\n\r\n    return {\r\n      fullUserPhotoUrl,\r\n      userName,\r\n      userId,\r\n    };\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAiBA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AAC9C,OAAOC,gBAAe,MAAO,iCAAiC;AAE9D,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIN,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMO,QAAO,GAAIP,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMQ,MAAK,GAAIR,GAAG,CAAC,EAAE,CAAC;IAEtB,MAAMS,gBAAe,GAAIR,QAAQ,CAAC,MAAM;MACtC,OAAOK,SAAS,CAACI,KAAI,IAAKP,gBAAgB;IAC5C,CAAC,CAAC;IAEF,MAAMQ,aAAY,GAAIA,CAAA,KAAM;MAC1B,MAAMC,EAAC,GAAIC,MAAM,CAACC,QAAQ,CAACC,MAAM;MAEjCH,EAAE,CAACI,KAAK,CAAC,MAAM;QACb,MAAMC,IAAG,GAAIL,EAAE,CAACM,cAAc,CAACD,IAAI;QACnC,IAAIA,IAAI,EAAE;UACRV,QAAQ,CAACG,KAAI,GAAIO,IAAI,CAACE,UAAS,IAAK,aAAa;UACjDX,MAAM,CAACE,KAAI,GAAIO,IAAI,CAACG,EAAC,IAAK,EAAE;UAC5BC,aAAa,CAACJ,IAAI,CAACK,SAAS,CAAC;UAC7BC,0BAA0B,CAACN,IAAI,CAACG,EAAE,EAAEH,IAAI,CAACE,UAAU,EAAEF,IAAI,CAACK,SAAS,CAAC;;UAEpE;UACAE,OAAO,CAACC,GAAG,CAAC,8BAA8B,EAAEb,EAAE,CAACM,cAAc,CAAC;QAChE;MACF,CAAC,CAAC;IACJ,CAAC;IAED,MAAMG,aAAY,GAAKK,QAAQ,IAAK;MAClC,IAAIA,QAAQ,EAAE;QACZpB,SAAS,CAACI,KAAI,GAAIgB,QAAQ;MAC5B,OAAO;QACLpB,SAAS,CAACI,KAAI,GAAIP,gBAAgB;MACpC;IACF,CAAC;IAED,MAAMoB,0BAAyB,GAAIA,CAACH,EAAE,EAAEhB,IAAI,EAAEsB,QAAQ,KAAK;MACzDC,YAAY,CAACC,OAAO,CAAC,kBAAkB,EAAER,EAAE,CAAC;MAC5CO,YAAY,CAACC,OAAO,CAAC,oBAAoB,EAAExB,IAAI,CAAC;MAChDuB,YAAY,CAACC,OAAO,CAAC,yBAAyB,EAAEF,QAAQ,CAAC;IAC3D,CAAC;IAEDxB,SAAS,CAAC,MAAM;MACdS,aAAa,CAAC,CAAC;IACjB,CAAC,CAAC;IAEF,OAAO;MACLF,gBAAgB;MAChBF,QAAQ;MACRC;IACF,CAAC;EACH;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}